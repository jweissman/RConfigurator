#!/usr/bin/env ruby

unless ARGV.count > 0
  puts "rconfig -- RConfiguration file verify"
  puts "USAGE: rconfig [file1] [file2] ... "
end

# require 'rubygems'				# needed with bundler/gem process?!?
# require 'rconfigurator'

ARGV.each do |config_file|
  RConfigurator.debug_config!("[Parent Module]", config_file, true)
end